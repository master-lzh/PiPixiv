{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": [
    "develop"
  ],
  "packageRules": [
    {
      "groupName": "所有依赖统一升级",
      "matchUpdateTypes": [
        "minor",
        "patch",
        "major"
      ],
      "matchPackagePatterns": [
        ".*"
      ]
    }
  ]
}
